{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "concurrently --kill-others-on-fail \"cd ../Back && venv/bin/uvicorn app.main:app --reload\" \"nuxt dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "oxlint src/**/*.ts src/**/*.vue",
    "lint:fix": "oxlint . src/**/*.ts src/**/*.vue --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\""
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.3.0",
    "@tailwindcss/vite": "^4.1.11",
    "@unovis/ts": "^1.5.2",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "nuxt": "^4.0.2",
    "nuxt-charts": "0.2.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.14"
  },
  "packageManager": "pnpm@9.5.0",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "oxlint": "^1.9.0",
    "prettier": "^3.6.2"
  }
}
