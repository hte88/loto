{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "concurrently --kill-others-on-fail \"cd ../Back && venv/bin/uvicorn app.main:app --reload\" \"nuxt dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "oxlint src/**/*.ts src/**/*.vue",
    "lint:fix": "oxlint . src/**/*.ts src/**/*.vue --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\""
 },
  "dependencies": {
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.3.0",
    "nuxt": "^4.0.1",
    "typescript": "^5.6.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "packageManager": "pnpm@9.5.0+sha1.8c155dc114e1689d18937974f6571e0ceee66f1d",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "oxlint": "^1.9.0",
    "prettier": "^3.6.2"
  }
}
